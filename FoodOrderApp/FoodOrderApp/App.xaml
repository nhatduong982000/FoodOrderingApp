<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FoodOrderApp.App">
    <Application.Resources>
        <Style x:Key="CabinSketch" TargetType="Label">
            <Setter Property="FontFamily" Value="cabinSketch_Bold.ttf"/>
        </Style>
        <Style x:Key="MyFont" TargetType="Label">
            <Setter Property="FontFamily" Value="cabinSketch_Regular.ttf"/>
        </Style>
    </Application.Resources>
</Application>