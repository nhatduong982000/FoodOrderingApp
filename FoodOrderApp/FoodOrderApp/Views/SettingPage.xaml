<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FoodOrderApp.Views.SettingPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Manage Categories" x:Name="ButtonCategories"
                    Clicked="ButtonCategories_Clicked"/>

            <Button Text="Magane Products" x:Name="ButtonProducts"
                    Clicked="ButtonProducts_Clicked"/>

            <Button Text="Magane Cart" x:Name="ButtonCart"
                    Clicked="ButtonCart_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>